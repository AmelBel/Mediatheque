package org.project.service;

import org.project.model.Document;
import org.project.model.User;

public interface IMediatheque {

	public Document consulterDocument(Document document); 
	public void effectuerEmprunt(User user, Document document); 
	public void restituerEmprunt(User user, Document document); 
	public void visualiserEmprunt(User user, Document document); 
	
}
